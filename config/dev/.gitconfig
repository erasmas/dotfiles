[user]
  name = Dmytro Morozov
  mail = mail@dmorozov.info
  email = mail@dmorozov.info
[github]
  user = erasmas
[core]
  excludesfile = ~/.gitignore_global
  autocrlf = input
[color]
  ui = true
[push]
  default = simple
[magit]
  hideCampaign = true
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  unstage = reset HEAD --
  last = log -1 HEAD
[init]
	defaultBranch = main
