colourscheme nord

set searchurls.go https://www.google.com/search?q=%s
set searchurls.trans https://translate.google.com/#auto/en/%s
set searchurls.so https://stackoverflow.com/search?q=%s
set searchurls.gh https://github.com/search?q=%s&type=Everything
set searchurls.rt https://www.rottentomatoes.com/search/?search=%s
set searchurls.yt https://www.youtube.com/results?search_query=%s
set searchurls.bc https://bandcamp.com/search?q=%s
set searchurls.dg https://www.discogs.com/search/?q=%s&type=all
set searchurls.lf https://www.last.fm/search?q=%s
set searchurls.gp https://play.google.com/store/search?q=%s
set searchurls.lgg https://www.light.gg/db/search/?q=%s
set searchurls.gr https://www.goodreads.com/search?q=%s

set editorcmd emacsclient -c %f

set allowautofocus false

set smoothscroll false
bind j scrollline 3
bind k scrollline -3

bind J tabnext
bind K tabprev
